}>\n        <div\n          className={`${classes.control} ${\n            emailState.isValid === false ? classes.invalid : ''\n          }`}\n        >\n          <label htmlFor=\"email\">E-Mail</label>\n          <input\n            type=\"email\"\n            id=\"email\"\n            value={emailState.value}\n            onChange={emailChangeHandler}\n            onBlur={validateEmailHandler}\n          />\n        </div>\n        <div\n          className={`${classes.control} ${\n            paswordState.isValid === false ? classes.invalid : ''\n          }`}\n        >\n          <label htmlFor=\"password\">Password</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            value={paswordState.value}\n            onChange={passwordChangeHandler}\n            onBlur={validatePasswordHandler}\n          />\n        </div>\n        <div className={classes.actions}>\n          <Button type=\"submit\" className={classes.btn} disabled={!formIsValid}>\n            Login\n          </Button>\n        </div>\n      </form>\n    </Card>\n  );\n};\n\nexport default Login;\n","C:\\Users\\msayyed\\Desktop\\scripts\\REACT\\refresher\\useEffect\\src\\components\\MainHeader\\Navigation.js",[],"C:\\Users\\msayyed\\Desktop\\scripts\\REACT\\refresher\\useEffect\\src\\components\\UI\\Button\\Button.js",[],"C:\\Users\\msayyed\\Desktop\\scripts\\REACT\\refresher\\useEffect\\src\\components\\UI\\Card\\Card.js",[],"C:\\Users\\msayyed\\Desktop\\scripts\\REACT\\refresher\\useEffect\\src\\context\\auth-context.js",[],{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":7,"column":3,"nodeType":"50","messageId":"51","endLine":10,"endColumn":4},{"ruleId":"48","severity":1,"message":"49","line":15,"column":3,"nodeType":"50","messageId":"51","endLine":18,"endColumn":4},"no-native-reassign",["52"],"no-negated-in-lhs",["53"],"default-case","Expected a default case.","SwitchStatement","missi